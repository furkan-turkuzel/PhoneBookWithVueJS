 axios from \"axios\"\r\nimport { ServerResponse } from \"http\";\r\n    export default {\r\n        props:[\"message\"],\r\n        components: {\r\n            BookRow,\r\n            PhoneAdd,\r\n            PhoneCall,\r\n            PhoneEdit\r\n        },\r\n        data() {\r\n            return {\r\n                newPerson: {\r\n                    id: 0,\r\n                    Title: \"\",\r\n                    Phone: \"\",\r\n                    IsFavorite:true\r\n                },\r\n                bookList:\r\n                    [\r\n                        \r\n                    ]\r\n            }\r\n        },\r\n        methods: {\r\n            AddBook($event) {\r\n\r\n                this.newPerson.Title = $event.Title\r\n                this.newPerson.Phone = $event.Phone\r\n\r\n                axios.post(\"https://phonebook-92502.firebaseio.com/bookList.json\", { Title: this.newPerson.Title, Phone: this.newPerson.Phone })\r\n                    .then(response => {\r\n                        console.log(response)\r\n                        this.bookList.push(this.newPerson)\r\n                    })\r\n                    .catch(e => {\r\n                        console.log(e)\r\n                    })\r\n\r\n                \r\n\r\n            }\r\n        },\r\n        created() {\r\n            axios.get(\"https://phonebook-92502.firebaseio.com/bookList.json\")\r\n                .then(response => {\r\n                    console.log(response.data)\r\n                    for (let key in response.data) {\r\n                        this.bookList.push(response.data[key])\r\n                    }\r\n                })\r\n                .catch(e => {\r\n                    console.log(e)\r\n                })\r\n        },\r\n        computed: {},\r\n        watch: {}\r\n    }\r\n</script>\r\n\r\n<style>\r\n    body {\r\n        background-image: url(\"../src/assets/bg.jpg\");\r\n        background-size: cover;\r\n        background-position: 